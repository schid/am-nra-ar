            <Article id="15348" parentID="15347" level="6" creatorID="0" sortOrder="0" createDate="2014-08-28T10:34:00" updateDate="2014-08-28T10:34:00" nodeName="Paired-up With Shotguns" urlName="paired-up-with-shotguns" path="-1,1133,2404,15114,15341,15347,15348" isDoc="" nodeType="1982" creatorName="Chester Campbell" writerName="Chester Campbell" writerID="0" template="1158" nodeTypeAlias="Article">
