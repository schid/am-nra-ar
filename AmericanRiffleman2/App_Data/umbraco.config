            <Article id="15217" parentID="15216" level="6" creatorID="0" sortOrder="0" createDate="2014-08-28T10:32:25" updateDate="2014-08-28T10:32:25" nodeName="Leupold Expands VX-6 Line" urlName="leupold-expands-vx-6-line" path="-1,1133,2404,15114,15134,15216,15217" isDoc="" nodeType="1982" creatorName="Chester Campbell" writerName="Chester Campbell" writerID="0" template="1158" nodeTypeAlias="Article">
