            <Article id="15117" parentID="15116" level="6" creatorID="0" sortOrder="0" createDate="2014-08-28T10:31:35" updateDate="2014-08-28T10:31:35" nodeName="AGI: Customizing the AR-15" urlName="agi-customizing-the-ar-15" path="-1,1133,2404,15114,15115,15116,15117" isDoc="" nodeType="1982" creatorName="Chester Campbell" writerName="Chester Campbell" writerID="0" template="1158" nodeTypeAlias="Article">
