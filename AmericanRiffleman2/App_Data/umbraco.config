            <Article id="15107" parentID="15091" level="6" creatorID="0" sortOrder="6" createDate="2014-08-28T10:25:12" updateDate="2014-08-28T10:25:12" nodeName="Ergo Grips Delta Grip For J-Frame Revolvers" urlName="ergo-grips-delta-grip-for-j-frame-revolvers" path="-1,1133,2404,15039,15040,15091,15107" isDoc="" nodeType="1982" creatorName="Chester Campbell" writerName="Chester Campbell" writerID="0" template="1158" nodeTypeAlias="Article">
